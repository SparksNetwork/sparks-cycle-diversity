{
  "name": "xstream-boilerplate",
  "version": "0.1.0",
  "description": "Sparks.network frontend",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TylorS/xstream-boilerplate.git"
  },
  "keywords": [
    "Cycle.js",
    "XStream"
  ],
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TylorS/xstream-boilerplate/issues"
  },
  "homepage": "https://github.com/TylorS/xstream-boilerplatey#readme",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "changelog": "conventional-changelog --infile CHANGELOG.md --same-file --release-count 0 --preset angular",
    "lint": "eslint src/*.js",
    "unit-test": "cross-env NODE_PATH=./src babel-node ./node_modules/.bin/mocha-webpack",
    "e2e-test": "cross-env LOCAL=true nightwatch",
    "sauce": "sc-run --id='Diversity' nightwatch --",
    "sauce:chrome49": "npm run sauce -- --env chrome49",
    "sauce:chrome50": "npm run sauce -- --env chrome50",
    "sauce:chrome51": "npm run sauce -- --env chrome51",
    "sauce:firefox46": "npm run sauce -- --env firefox46",
    "sauce:firefox47": "npm run sauce -- --env firefox47",
    "sauce:ie9": "npm run sauce -- --env ie9",
    "sauce:ie10": "npm run sauce -- --env ie10",
    "sauce:ie11": "npm run sauce -- --env ie11",
    "sauce:edge": "npm run sauce -- --env edge",
    "sauce:android42": "npm run sauce -- --env android42",
    "sauce:android44": "npm run sauce -- --env android44",
    "sauce:android51": "npm run sauce -- --env android51",
    "sauce:iphone5": "npm run sauce -- --env iphone5",
    "sauce:iphone6": "npm run sauce -- --env iphone6",
    "sauce:iphone6plus": "npm run sauce -- --env iphone6plus",
    "sauce:safari7": "npm run sauce -- --env safari7",
    "sauce:safari8": "npm run sauce -- --env safari8",
    "sauce:safari9": "npm run sauce -- --env safari9",
    "test": "npm run lint && npm run unit-test && npm run e2e-test",
    "test-ci": "npm run lint && npm run unit-test && npm run sauce -- --env chrome49,firefox46,ie10,android44,iphone5,safari7",
    "serve-static": "pushstate-server dist/ 8080 /200.html",
    "serve": "cross-env babel-node ./node_modules/.bin/webpack-dev-server",
    "build": "cross-env babel-node ./node_modules/.bin/webpack"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "node ./node_modules/validate-commit-msg/index.js"
    }
  },
  "dependencies": {
    "@cycle/dom": "^10.0.5",
    "@cycle/isolate": "^1.4.0",
    "@cycle/xstream-adapter": "^3.0.2",
    "@cycle/xstream-run": "^3.0.3",
    "attr-accept": "^1.0.3",
    "braintree-web": "^2.26.0",
    "cropperjs": "^0.7.2",
    "cyclic-router": "^2.1.2",
    "es6-shim": "^0.35.1",
    "firebase": "^3.1.0",
    "moment": "^2.13.0",
    "node-polyglot": "^2.0.0",
    "normalize-css": "^2.3.1",
    "ramda": "^0.21.0",
    "xstream": "^5.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-espower": "^2.3.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chromedriver": "^2.21.2",
    "commitizen": "^2.8.2",
    "conventional-changelog": "^1.1.0",
    "conventional-changelog-cli": "^1.2.0",
    "coveralls": "^2.11.9",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-loader": "^1.4.0",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-standard": "^1.3.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "ghooks": "^1.3.0",
    "image-webpack-loader": "^1.8.0",
    "isparta": "^4.0.0",
    "mocha": "^2.5.3",
    "mocha-circleci-reporter": "0.0.1",
    "mocha-webpack": "^0.4.0",
    "nightwatch": "^0.9.5",
    "node-sass": "^3.8.0",
    "nodemon": "^1.9.2",
    "postcss-loader": "^0.9.1",
    "power-assert": "^1.4.1",
    "pushstate-server": "^1.8.1",
    "style-loader": "^0.13.1",
    "surge": "^0.18.0",
    "validate-commit-msg": "^2.6.1",
    "wait-on": "^1.5.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-node-externals": "^1.2.0"
  }
}
